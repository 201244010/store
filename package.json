{
	"name": "ant-design-pro",
	"version": "2.1.1",
	"description": "An out-of-box UI solution for enterprise applications",
	"private": true,
	"scripts": {
		"start": "cross-env APP_TYPE=site umi dev",
		"start:no-mock": "cross-env MOCK=none umi dev",
		"dev": "cross-env UMI_ENV=dev APP_TYPE=site umi dev",
		"dev:south-america": "cross-env UMI_ENV=dev APP_TYPE=site COUNTRY=southAmerica umi dev",
		"dev:test": "cross-env UMI_ENV=test APP_TYPE=site umi dev",
		"dev:local": "cross-env UMI_ENV=local APP_TYPE=site umi dev",
		"dev:uat": "cross-env UMI_ENV=uat APP_TYPE=site umi dev",
		"dev:onl": "cross-env UMI_ENV=onl APP_TYPE=site umi dev",
		"dev:no-mock": "cross-env MOCK=none umi dev",
		"build:dev": "cross-env UMI_ENV=dev APP_TYPE=site umi build",
		"build:test": "cross-env UMI_ENV=test APP_TYPE=site umi build",
		"build:uat": "cross-env UMI_ENV=uat APP_TYPE=site umi build",
		"build:onl": "cross-env UMI_ENV=onl APP_TYPE=site umi build",
		"build:local": "cross-env UMI_ENV=local APP_TYPE=site umi build",
		"build:demo": "cross-env UMI_ENV=local APP_TYPE=site umi build",
		"analyze": "cross-env ANALYZE=1 umi build",
		"lint:style": "stylelint \"src/**/*.less\" --syntax less",
		"lint": "eslint --ext .js src mock tests && npm run lint:style",
		"lint:fix": "eslint --fix --ext .js src mock tests && npm run lint:style",
		"lint-staged": "lint-staged",
		"lint-staged:js": "eslint --ext .js",
		"prettier": "node ./scripts/prettier.js",
		"test": "npx jest --colors --coverage"
	},
	"dependencies": {
		"@antv/data-set": "^0.9.6",
